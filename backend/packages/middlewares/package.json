{
  "name": "@pharma/middlewares",
  "version": "1.0.0",
  "packageManager": "pnpm@9.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "@pharma/utils": "workspace:*",
    "@pharma/infra": "workspace:*",
    "@pharma/libs": "workspace:*",
    "@pharma/core": "workspace:*"
  }
}
