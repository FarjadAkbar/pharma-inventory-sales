{
  "name": "@pharma/infra",
  "version": "1.0.0",
  "packageManager": "pnpm@9.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "require": "./dist/*.js",
      "import": "./dist/*.js",
      "default": "./dist/*.js"
    },
    "./*/*": {
      "types": "./dist/*/*.d.ts",
      "require": "./dist/*/*.js",
      "import": "./dist/*/*.js",
      "default": "./dist/*/*.js"
    },
    "./*/*/*": {
      "types": "./dist/*/*/*.d.ts",
      "require": "./dist/*/*/*.js",
      "import": "./dist/*/*/*.js",
      "default": "./dist/*/*/*.js"
    },
    "./*/*/*/*": {
      "types": "./dist/*/*/*/*.d.ts",
      "require": "./dist/*/*/*/*.js",
      "import": "./dist/*/*/*/*.js",
      "default": "./dist/*/*/*/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": ["./dist/*/index.d.ts", "./dist/*.d.ts"],
      "*/*": ["./dist/*/*/index.d.ts", "./dist/*/*.d.ts"],
      "*/*/*": ["./dist/*/*/*/index.d.ts", "./dist/*/*/*.d.ts"],
      "*/*/*/*": ["./dist/*/*/*/*/index.d.ts", "./dist/*/*/*/*.d.ts"]
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "@pharma/utils": "workspace:*"
  }
}
